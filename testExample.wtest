import example.*

describe "pruebas de dominic" {
  test "dominic compra superPerrari y ferrari, los repara y trabaja" {
    dominic.comprar(ferrari)
    dominic.comprar(superPerrari)
    dominic.probarVelocidad()
    dominic.trabajar()
    assert.equals(dominic.autoMasRapido(), superPerrari)
  }

}